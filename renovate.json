{
  "pinDigest": true,
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": ["^Dockerfile$"],
      "matchStrings": [
        "FROM python:(?<verCompatibility>.*)-(?<currentValue>.+)@(?<currentDigest>sha256:.+)"
      ],
      "depNameTemplate": "python",
      "datasourceTemplate": "docker",
      "versioningTemplate": "debian",
      "extractVersionTemplate": "^{{verCompatibility}}-(?<version>[a-z]+)$"
    },
    {
      "customType": "regex",
      "fileMatch": ["^Dockerfile$"],
      "matchStrings": [
        "FROM python:(?<currentValue>.+)-(?<verCompatibility>.*)@(?<currentDigest>sha256:.+)"
      ],
      "depNameTemplate": "python",
      "datasourceTemplate": "docker",
      "versioningTemplate": "python",
      "extractVersionTemplate": "^(?<version>.+)-{{verCompatibility}}$"
    }
  ]
}